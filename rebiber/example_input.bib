  mendeley-groups = {Multilingual,general NLP},
  month           = {nov},
  pages           = {4411--4421},
  publisher       = {PMLR},
  title           = {{XTREME: A Massively Multilingual Multi-task Benchmark for Evaluating Cross-lingual Generalization}},
